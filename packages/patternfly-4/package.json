{
  "name": "@patternfly/org-patternfly-4",
  "description": "Documentation for PatternFly 4",
  "version": "1.0.0",
  "author": "Red Hat",
  "scripts": {
    "build:babel": "cross-env babel _repos/react-core/src --out-dir dist/esm --extensions \".js,jsx,.ts,.tsx\"",
    "build": "yarn netlify-cache && yarn gen-ts-docs && node --max_old_space_size=4096 ./node_modules/.bin/gatsby build --prefix-paths",
    "build:root": "yarn gen-ts-docs && node --max_old_space_size=4096 ./node_modules/.bin/gatsby build",
    "develop": "yarn gen-ts-docs && gatsby develop",
    "netlify-cache": "node ./scripts/netlify-cache.js",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "node ./scripts/clean.js",
    "repos": "yarn clean && yarn patternfly-next && yarn patternfly-react && yarn patternfly-styles",
    "patternfly-next": "node ./scripts/patternfly-next.js",
    "patternfly-react": "node ./scripts/patternfly-react.js",
    "patternfly-styles": "node ./scripts/copyStyles.js",
    "gen-ts-docs": "typedoc --json ts-docs/ts-output.json ./_repos/react-core/src --ignoreCompilerErrors --mode file --tsconfig ./_repos/react-core/src/tsconfig.dts.json && node scripts/transformTsDocs.js",
    "preview-screenshots": "node ./scripts/previewScreenshots.js"
  },
  "dependencies": {
